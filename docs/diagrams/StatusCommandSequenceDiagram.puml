@startuml
!include style.puml
skinparam ArrowFontStyle plain

box External #white
actor User as user
end box

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as logic LOGIC_COLOR
participant ":AddressBookParser" as abp LOGIC_COLOR
participant ":SetStatusCommandParser" as sscp LOGIC_COLOR
participant ":SetStatusCommand" as ssc LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as model MODEL_COLOR
participant ":Person" as person MODEL_COLOR
end box

user -> logic : execute("status 1 Contacted")
activate logic
logic -> abp : parseCommand("status 1 Contacted")
activate abp
abp -> sscp : parse("1 Contacted")
activate sscp
create ssc
sscp -> ssc : SetStatusCommand(index, status)
activate ssc
ssc --> sscp
deactivate ssc
sscp --> abp : SetStatusCommand
deactivate sscp
abp --> logic : SetStatusCommand
deactivate abp
logic -> ssc : execute(model)
activate ssc
ssc -> model : getFilteredPersonList()
activate model
model --> ssc : personList
deactivate model
ssc -> model : getPerson(index)
activate model
model --> ssc : personToEdit
deactivate model
create person
ssc -> person : Person(updatedPerson)
activate person
person --> ssc
deactivate person
ssc -> model : setPerson(personToEdit, editedPerson)
activate model
model --> ssc : void
deactivate model
ssc -> model : updateFilteredPersonList(PREDICATE_SHOW_ALL_PERSONS)
activate model
model --> ssc : void
deactivate model
ssc --> logic : CommandResult
deactivate ssc
logic --> user : "Updated status of person to: Contacted"
deactivate logic
@enduml
